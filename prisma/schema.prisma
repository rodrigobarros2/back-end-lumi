generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Fatura {
    id            String @id @default(cuid())
    numeroCliente String
    mesReferencia String

    energiaEletrica   Json
    energiaScee       Json
    energiaCompensada Json

    contribilum Float

    faturaURL String?

    createdAt DateTime  @default(now())
    updatedAt DateTime?
}
